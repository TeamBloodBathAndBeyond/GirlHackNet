DROP TABLE hackathon;
CREATE TABLE hackathon(
   id       INTEGER  NOT NULL PRIMARY KEY 
  ,name     VARCHAR(20) NOT NULL
  ,date     VARCHAR(30) NOT NULL
  ,link     VARCHAR(50) NOT NULL
  ,location VARCHAR(30) NOT NULL
  ,is_US		BOOL NOT NULL
);

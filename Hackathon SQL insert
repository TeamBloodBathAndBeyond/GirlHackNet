CREATE TABLE hackathon(
   id       INTEGER  NOT NULL PRIMARY KEY 
  ,name     VARCHAR(30) NOT NULL
  ,date     VARCHAR(40) NOT NULL
  ,link     VARCHAR(100) NOT NULL
  ,location VARCHAR(30) NOT NULL
  ,is_US    BOOLEAN  NOT NULL
);
